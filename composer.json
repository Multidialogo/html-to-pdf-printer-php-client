{
  "name": "multidialogo/html-to-pdf-printer-php-client",
  "description": "Client for the html to pdf service.",
  "type": "library",
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MultiDialogo\\HtmlToPdfPrinterPhpClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MultiDialogo\\HtmlToPdfPrinterPhpClient\\Tests\\": "test/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Michele Carino",
      "email": "carino@multidialogo.it"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
